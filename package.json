{
  "title": "Jarvis Annotator",
  "name": "jarvis-addon",
  "version": "0.0.6",
  "description": "A Jarvis add-on for managing annotations",
  "main": "index.js",
  "author": "Tooling",
  "engines": {
    "firefox": ">=38.0a1",
    "fennec": ">=38.0a1"
  },
  "license": "MIT",
  "dependencies": {
    "jira-module": "file:data/jira-module"
  },
  "preferences": [
    {
      "description": "JIRA address",
      "type": "string",
      "name": "jiraHost",
      "value": "https://jira.subutai.io",
      "title": "JIRA address"
    },
    {
      "label": "Apply",
      "type": "control",
      "name": "applyChanges",
      "title": "Apply changes"
    }
  ]
}
